{
  "name": "claudex-docs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "sync-schemas": "rm -rf public/schemas && cp -r ../schemas public/schemas",
    "dev": "pnpm sync-schemas && astro dev",
    "build": "pnpm sync-schemas && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild", "sharp"]
  },
  "dependencies": {
    "astro": "^5.7.0",
    "@astrojs/starlight": "^0.34.0",
    "@astrojs/sitemap": "^3.3.0",
    "@astrojs/starlight-tailwind": "^4.0.0",
    "tailwindcss": "^4.1.0",
    "@tailwindcss/vite": "^4.1.0",
    "sharp": "^0.33.0"
  }
}
